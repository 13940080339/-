package TEST;

import org.junit.Test;

import static org.junit.Assert.*;

public class MainTest {

    @Test
    public void test1() {
          int n=2;
          int  []a={-1,-2};
          assertEquals(0,Main.add(n,a));
    }
    @Test
    public void test2() {
        int n=3;
        int  []a={1,2,3};
        assertEquals(6,Main.add(n,a));
    }
    @Test
    public void test3() {
        int n=3;
        int  []a={1,2,-1};
        assertEquals(3,Main.add(n,a));
    }
}
